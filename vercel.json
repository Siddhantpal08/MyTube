{
    "version": 2,
    "builds": [
      {
        "src": "Backend/src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/(.*)",
        "dest": "Backend/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/Frontend/dist/$1"
      }
    ]
  }
  