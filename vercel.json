{
  "version": 2,
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "Backend/src/index.js"
    },
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "s-maxage=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}